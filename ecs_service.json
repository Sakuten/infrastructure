[
  {
    "name": "sakuten_backend",
    "image": "389602053363.dkr.ecr.ap-northeast-1.amazonaws.com/sakuten",
    "cpu": 10,
    "memory": 512,
    "essential": true,
    "entryPoint": [],
    "environment": [
      {
        "name": "FLASK_CONFIGURATION",
        "value": "deployment"
      }, {
        "name": "SECRET_KEY",
        "value": "${secret_key}"
      }, {
        "name": "DATABASE_URL",
        "value": "${database_url}"
      }, {
        "name": "RECAPTCHA_SECRET_KEY",
        "value": "${recaptcha_secret_key}"
      }
    ],
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ]
  }
]
